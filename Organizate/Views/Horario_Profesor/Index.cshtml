@model IEnumerable<Organizate.Horario_Profesor>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<div id="page-wrapper">
	<h2>Index</h2>

	<p>
		@Html.ActionLink("Create New", "Create")
	</p>
	<table class="table">
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.hor_pro_dia)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.hor_pro_hora_inicio)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.hor_pro_hora_fin)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Profesor.pro_nombre)
			</th>
			<th></th>
		</tr>

		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.hor_pro_dia)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.hor_pro_hora_inicio)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.hor_pro_hora_fin)
				</td>
				<td>
					@Html.DisplayFor(modelItem => item.Profesor.pro_nombre)
				</td>
				<td>
					@Html.ActionLink("Edit", "Edit", new { id = item.hor_pro_id }) |
					@Html.ActionLink("Details", "Details", new { id = item.hor_pro_id }) |
					@Html.ActionLink("Delete", "Delete", new { id = item.hor_pro_id })
				</td>
			</tr>
		}

	</table>
	</div>